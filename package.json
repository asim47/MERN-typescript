{
  "name": "react-typescript-asim-webpack",
  "version": "1.0.0",
  "description": "MERN stack with typescript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config webpack.dev.js --open ",
    "prod": "webpack --config webpack.prod.js --watch",
    "build": "webpack --config webpack.prod.js",
    "node": "nodemon server/server.js --ignore 'dist/' --ignore 'src/'",
    "prod:node": "concurrently \"npm run prod\" \"npm run node\"",
    "dev:node": "concurrently \"npm run dev\" \"npm run node\"",
    "start": "node server/server.js"
  },
